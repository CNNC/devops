apiVersion: networking.k8s.io/v1beta1
kind: Ingress
metadata:
  annotations:
    ingress.cloud.tencent.com/cross-region-id: ap-nanjing
    ingress.cloud.tencent.com/cross-type: PVGW
    ingress.cloud.tencent.com/cross-vpc-id: vpc-fnzzdvjn
    ingress.cloud.tencent.com/snat-pro-info: '{"snatIps":[{"subnetId":"subnet-bhyvjxmq"}]}'
    ingress.kubernetes.io/tke-existed-lbid: lb-238tvmx6
  name: nj-cross-sh-ingress-80
  namespace: prod
spec:
  rules:
  - http:
      paths:
      - backend:
          serviceName: nj-cross-sh-service
          servicePort: 80
        path: /
