apiVersion: networking.k8s.io/v1beta1
kind: Ingress
metadata:
  annotations:
    ingress.cloud.tencent.com/cross-region-id: ap-tianjin
    ingress.cloud.tencent.com/cross-type: PVGW
    ingress.cloud.tencent.com/cross-vpc-id: vpc-eb3o1x0c
    ingress.cloud.tencent.com/snat-pro-info: '{"snatIps":[{"subnetId":"subnet-n4fdw45n"}]}'
    ingress.kubernetes.io/tke-existed-lbid: lb-ma2trnmr
  name: tj-cross-bj-ingress-80
  namespace: prod
spec:
  rules:
  - http:
      paths:
      - backend:
          serviceName: tj-cross-bj-service
          servicePort: 80
        path: /